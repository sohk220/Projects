define service{
        use                             dc-fork-service         ; Name of service template to use
	hostgroup_name			dc-fork-nas,dc-fork-mdc,dc-fork-od,dc-fork-mpr,dc-fork-pds,dc-fork-msb
        service_description             PING
        normal_check_interval           1
        contacts	                dcadmin
	notification_interval		120
        notification_options            w,c,r
        check_command                   check_ping!100.0,20%!500.0,60%
}
