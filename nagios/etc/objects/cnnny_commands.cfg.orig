# 'check_http' command definition -checks MS2 on port 8080
define command{
        command_name    check_http_msny_8080
        command_line    $USER1$/check_http -H $HOSTADDRESS$ -p 8080 $ARG1$
}

define command{
        command_name    check_remote_disk
