# 'check_win_disk_c' command definition - uses Check WMI Plus tools
# This checks used space. -w 80 -c 90 will warn at 80% used and bark crit at 90%
define command{
        command_name    check_win_disk
        command_line    $USER1$/check_wmi_plus.pl -H $HOSTADDRESS$ -m checkdrivesize -u $USER8$ -p $USER7$ -a d: -o 1 -w 90 -c 95 $USER5$
}

#Check MS2-DB Disk Space definition
